<?xml version="1.0" encoding="utf-8"?>
<uiManifest>
  <!--Billing-->
  <extension name="UploadTenantExtension"
             baseUri="~/Content/UploadTenant">

    <scripts>
      <script src="~/Scripts/UploadTenant.UploadRecordsTab.js" />
      <script src="~/Scripts/UploadTenant.UploadCurrentTab.js" />
      <script src="~/Scripts/UploadTenant.Controller.js" />
      <script src="~/Scripts/UploadTenant.SettingsTab.js" />
      <script src="~/UploadTenantExtension.js" />

      <!--Self registration-->
      <script src="~/extensions.data.js" />
    </scripts>
    <stylesheets>
      <stylesheet src="~/Styles/UploadTenant.css"/>
    </stylesheets>
    <templates>
      <template name="uploadRecordsTab"
               src="~/Templates/Tabs/UploadRecordsTab.html" />
      <template name="uploadCurrentTab"
                src="~/Templates/Tabs/UploadCurrentTab.html" />     
      <template name="uploadRecordsTabEmpty"
                src="~/Templates/Tabs/UploadRecordsTabEmpty.html" />
      <template name="settingsTab"
                src="~/Templates/Tabs/SettingsTab.html" />
      <template name="FileUploadWizard"
               src="~/Templates/Tabs/FileUploadWizard.html" />
    </templates>
    
  </extension>
</uiManifest>
